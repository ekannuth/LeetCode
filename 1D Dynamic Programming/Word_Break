class Solution:
	def wordBreak(self, s, wordDict):
		leftp = 0
		rightp = 1
		Valid = False
		while rightp <= len(s):
			if s[rightp - 1] in wordDict:
				Valid = True
				rightp += 1
			elif s[leftp:rightp] in wordDict:
				Valid = True
				rightp += 1
			elif leftp == rightp - 1:
				Valid = False
				rightp += 1
			elif Valid == True:
				leftp += 1
			else:
				rightp += 1
		print(Valid)
		return Valid
		
	

words = Solution()
wordDict = {
	"aaaa": 2,
	"aa": 3,
}

words.wordBreak("aaaaaaa", wordDict)
