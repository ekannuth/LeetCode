class Solution:
	def wordBreak(self, s, wordDict):
		word = s[0]
		for i in range(1,len(s)):
			if word in wordDict:
				word = s[i]
				print(word)
				print(s[i:])
			else:
				word += s[i]
		print(word)
		if word in wordDict:
			print(True)
			return True
		else:
			print(False)
			return False
			

words = Solution()
wordDict = { 
	"aaaa": 1,
	"aaa": 2
}
words.wordBreak("aaaaaaa", wordDict)
