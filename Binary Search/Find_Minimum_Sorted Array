class Solution(object):
    def findMin(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        left = 0
        right = len(nums) - 1
        mid = (left + right) // 2
        minimum_val = nums[mid]
        while (right >= left):
            mid = (left + right) // 2
            if minimum_val > nums[mid]:
                minimum_val = nums[mid]
                continue
            elif nums[mid] < nums[left]:
                right = mid - 1
            elif nums[mid] > nums[right]:
                left = mid + 1
            else:
                right = mid - 1
        return minimum_val
